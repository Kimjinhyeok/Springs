<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.test.tiles.mapper.PositionMapper">
	<insert id="insertPosition" parameterType="com.test.tiles.vo.PositionsVO">
		INSERT INTO 
		MENUS(
			posName
		)VALUES(
			#{posName}
		)
	</insert>
	
	<select id="getPositionList" resultType="com.test.tiles.vo.PositionsVO">
		SELECT * FROM Positions ORDER BY POSIDX ASC;
	</select>
</mapper>